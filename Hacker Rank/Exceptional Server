		/* Enter your code here. */
        try{
            int x=Server::compute(A, B);
            cout << x << endl;
        }
        catch(bad_alloc& e){
            cout << "Not enough memory"<< endl;
        }
        catch(exception &e){
	        cout << "Exception: " << e.what() << endl;
        }
        catch(...){
	        cout<<"Other Exception"<<'\n';
